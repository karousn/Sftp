{
    "name": "ucsdmath/sftp",
    "type": "library",
    "description": "Sftp is a adapter for the phpseclib library. Sftp is a testing and development library only",
    "keywords": ["sftp", "ucsdmath", "phpseclib", "component"],
    "homepage": "https://github.com/ucsdmath",
    "license": "MIT",
    "time": "2016-03-01",
    "authors": [
        {"name": "Daryl Eisner", "email": "deisner@ucsd.edu"}
    ],
    "support": {
        "source": "https://github.com/UCSDMath/Sftp",
        "email": "deisner@ucsd.edu"
    },
    "require": {
        "php": "^7.0",
        "phpseclib/phpseclib": "2.0.1",
        "ucsdmath/functions": "1.7.x-dev",
        "ucsdmath/configuration": "1.7.x-dev",
        "ucsdmath/dependency-injection": "1.7.x-dev"
    },
    "require-dev": {
        "mockery/mockery": "0.9.4",
        "phpunit/phpunit": "5.3.2",
        "ucsdmath/testing": "1.7.x-dev",
        "squizlabs/php_codesniffer": "2.6.0"
    },
    "autoload": {
        "psr-4": {
            "UCSDMath\\Sftp\\": "src/Sftp/"
        }
    },
    "suggest": {
        "symfony/filesystem": "Filesystem provides basic utility to manipulate the file system."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.7.x-dev"
        }
    }
}
