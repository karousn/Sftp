{
    "name": "ucsdmath/sftp",
    "type": "library",
    "description": "Sftp is a adapter for the phpseclib library. Sftp is a testing and development library only",
    "keywords": ["sftp", "ucsdmath", "phpseclib", "component"],
    "homepage": "https://github.com/ucsdmath",
    "license": "MIT",
    "time": "2016-07-03",
    "authors": [
        {
            "name": "Daryl Eisner",
            "email": "deisner@ucsd.edu"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "phpseclib/phpseclib": "2.0.2",
        "ucsdmath/functions": "1.8.x-dev",
        "ucsdmath/configuration": "1.8.x-dev",
        "ucsdmath/dependency-injection": "1.8.x-dev"
    },
    "require-dev": {
        "mockery/mockery": "0.9.5",
        "phpunit/phpunit": "5.4.7",
        "ucsdmath/testing": "1.8.x-dev",
        "squizlabs/php_codesniffer": "2.6.2"
    },
    "autoload": {
        "psr-4": {
            "UCSDMath\\Sftp\\": "src/Sftp/"
        }
    },
    "suggest": {
        "symfony/filesystem": "Filesystem provides basic utility to manipulate the file system."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.8-dev"
        }
    },
    "minimum-stability": "dev"
}
