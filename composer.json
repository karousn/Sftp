{
    "name": "ucsdmath/sftp",
    "type": "library",
    "description": "Sftp is a adapter for the phpseclib library. Sftp is a testing and development library only.",
    "keywords": ["sftp", "ucsdmath", "phpseclib", "component"],
    "homepage": "https://github.com/ucsdmath",
    "license": "MIT",
    "time": "2017-03-31",
    "authors": [
        {
            "name": "Daryl Eisner",
            "email": "deisner@ucsd.edu"
        }
    ],
    "require": {
        "php": ">=7.1",
        "phpseclib/phpseclib": "2.0.4",
        "ucsdmath/functions": "1.18.x-dev",
        "ucsdmath/configuration": "1.18.x-dev",
        "ucsdmath/dependency-injection": "1.18.x-dev"
    },
    "require-dev": {
        "symfony/debug": "3.2.7",
        "mockery/mockery": "0.9.9",
        "phpunit/phpunit": "6.0.13",
        "fzaninotto/faker": "1.6.0",
        "ucsdmath/date": "1.18.x-dev",
        "symfony/var-dumper": "3.2.7",
        "symfony/browser-kit": "3.2.7",
        "symfony/dom-crawler": "3.2.7",
        "symfony/options-resolver": "3.2.7",
        "squizlabs/php_codesniffer": "2.8.1"
    },
    "autoload": {
        "psr-4": {
            "UCSDMath\\Sftp\\": "src/Sftp/"
        }
    },
    "suggest": {
        "symfony/filesystem": "Filesystem provides basic utility to manipulate the file system."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.18-dev"
        }
    },
    "minimum-stability": "dev"
}
